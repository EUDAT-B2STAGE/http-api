// B2SAFE REST API configuration
{
    // enabling B2ACCESS token
    "custom.oauth": {
        "OauthLogin": [
            "askauth"
        ],
        "Authorize": [
            "authorize"
        ]
    },
    // upload and download for normal users
    "custom.eudat": {
        //// This is now called AGGREGATION and are encapsuled inside
        //// the digitalentity concept
        // "CollectionEndpoint": [
        //     "collections",
        //     "collections/<string:uuid>"
        // ],
        "DigitalEntityEndpoint": [
            "digitalentity",
            "digitalentity/<string:uuid>"
        ]
    },
    "custom.internals.b2safe": {
        "MetaDataObject": [
            "internals/mdstore",
            "internals/mdstore/<string:uuid>"
        ]
    }
}
